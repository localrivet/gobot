# Gobot Frontend Environment
# Copy to .env.local for local development
#
# NOTE: Branding/SEO is configured in src/lib/config/site.ts (not here)
# NOTE: API URL auto-detected from browser origin (embedded static frontend)

# =============================================================================
# Monitoring (Optional)
# =============================================================================

# Environment (development, staging, production)
VITE_ENVIRONMENT=development

# Application version (set during CI/CD)
VITE_APP_VERSION=

# Enable debug mode
VITE_ENABLE_DEBUG=false

# Alert webhook URL (Slack, Discord, or custom endpoint)
VITE_ALERT_WEBHOOK_URL=
